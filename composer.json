{
    "name": "laravel/laravel",
    "type": "project",
    "description": "O skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^3.0",
        "beyondcode/laravel-websockets": "^1.14",
        "erusev/parsedown": "^1.7",
        "google/cloud-vision": "^1.10",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/broadcasting": "*",
        "laravel/framework": "^10.10",
        "laravel/reverb": "^1.4",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "livepixel/mercado-pago": "^1.0",
        "livewire/livewire": "^3.6",
        "maatwebsite/excel": "^3.1",
        "mckenziearts/laravel-notify": "^2.6",
        "mercadopago/dx-php": "3.0.8",
        "mikehaertl/php-pdftk": "^0.13.0",
        "nesbot/carbon": "^2.67",
        "phpoffice/phpspreadsheet": "^1.29",
        "phpoffice/phpword": "^1.4",
        "pusher/pusher-php-server": "^7.2",
        "react/http": "^1.8",
        "react/promise": "2.8.0",
        "realrashid/sweet-alert": "^7.0",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.3",
        "smalot/pdfparser": "^2.5",
        "spatie/laravel-medialibrary": "^10.15",
        "spatie/pdf-to-text": "^1.52",
        "symfony/css-selector": "^7.3",
        "symfony/dom-crawler": "^7.3",
        "tecnickcom/tcpdf": "^6.6",
        "thiagoalessio/tesseract_ocr": "^2.13"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.21",
        "laravel/pint": "^1.21",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
